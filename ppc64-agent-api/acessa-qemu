which qemu-system-ppc64

qemu-system-ppc64 \
  -m 2048 \
  -hda /opt/homebrew/bin/qemu-system-ppc64 \
  -cpu POWER8 \
  -M pseries \
  -nographic \
  -serial mon:stdio

